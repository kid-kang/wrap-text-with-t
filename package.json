{
  "name": "wrap-text-with-t",
  "displayName": "Wrap Text With T",
  "description": "Function shortcut package selection content",
  "version": "0.0.1",
  "publisher": "kid-kang",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.wrapTextWithT"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.wrapTextWithT",
        "title": "国际化函数包裹"
      }
    ],
    "keybindings": [
      {
        "command": "extension.wrapTextWithT",
        "key": "ctrl+alt+z",
        "mac": "cmd+alt+z",
        "when": "editorTextFocus && editorHasSelection"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}